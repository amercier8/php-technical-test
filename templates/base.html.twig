<body>
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>
<nav>
    <div class="nav-wrapper">
        <a href="#!" class="brand-logo center">Test Stadline</a>
        <ul class="left hide-on-med-and-down">
            {% if app.user %}
                <li><a href="/running/consult">Vos courses</a></li>
                <li><a href="/running/add">Nouvelle course</a></li>
            {% endif %}
        </ul>
        <ul class="right hide-on-med-and-down">
            {% if app.user == false %}
                <li><a class="black-text text-darken-2" href="/running/register">S'inscrire</a></li>
                <li><a class="black-text text-darken-2" href="/running/login">Se connecter</a></li>
            {% else %}
                <li><a class="black-text text-darken-2" href="/running/logout">Se d√©connecter</a></li>
            {% endif %}
        </ul>
    </div>
</nav>
{% block body %}
{% endblock %}
{% block javascripts %}{% endblock %}
{% block flash_messages %}
    {% for message in app.flashes('notice') %}
        <div class="flash-card-panel teal lighten-2 example-wrapper">
            {{ message }}
        </div>
    {% endfor %}
{% endblock %}
</body>

